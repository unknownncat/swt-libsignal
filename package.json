{
  "name": "@unknownncat/swt-libsignal",
  "version": "2.0.0",
  "description": "Reimplementação do protocolo Signal usando libsodium.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/unknownncat/swt-libsignal/",
  "repository": {
    "url": "https://github.com/unknownncat/swt-libsignal.git"
  },
  "author": "unknownncat",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./libsignal": {
      "types": "./dist/compat/libsignal/index.d.ts",
      "import": "./dist/compat/libsignal/index.js"
    },
    "./src/crypto": {
      "types": "./dist/compat/libsignal/src/crypto.d.ts",
      "import": "./dist/compat/libsignal/src/crypto.js"
    },
    "./src/curve": {
      "types": "./dist/compat/libsignal/src/curve.d.ts",
      "import": "./dist/compat/libsignal/src/curve.js"
    },
    "./src/keyhelper": {
      "types": "./dist/compat/libsignal/src/keyhelper.d.ts",
      "import": "./dist/compat/libsignal/src/keyhelper.js"
    },
    "./src/protobufs": {
      "types": "./dist/compat/libsignal/src/protobufs.d.ts",
      "import": "./dist/compat/libsignal/src/protobufs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && cpy \"src/types/**/*.{ts,d.ts}\" dist/types && cpy \"src/public/*.mjs\" dist/public",
    "postinstall": "node scripts/check-engine.js",
    "clean": "rimraf dist",
    "test": "cross-env NODE_OPTIONS=--expose-gc vitest run",
    "test:watch": "cross-env NODE_OPTIONS=--expose-gc vitest",
    "test:coverage": "cross-env NODE_OPTIONS=--expose-gc vitest run --coverage",
    "test:interop": "cross-env NODE_OPTIONS=--expose-gc vitest run test/integration/libsignal-node-interop.test.ts",
    "test:fuzz": "cross-env RUN_FUZZ=1 NODE_OPTIONS=--expose-gc vitest run test/fuzz/protocol-fuzz.test.ts",
    "proto:gen": "npx --no-install pbjs -t static-module -w es6 -o src/proto/generated/whisper-text-protocol.js protos/WhisperTextProtocol.proto && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/proto/generated --ts_proto_opt=esModuleInterop=true,forceLong=number,useOptionals=all protos/WhisperTextProtocol.proto",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=high",
    "test:exports": "tsx scripts/check-api-exports.ts",
    "test:exports:update": "tsx scripts/check-api-exports.ts --write",
    "benchmark": "cross-env NODE_OPTIONS=--expose-gc tsx scripts/bench-job-queue.ts",
    "examples:run": "tsx docs/examples/run-all.ts"
  },
  "engines": {
    "node": ">=20 <21 || >=22"
  },
  "dependencies": {
    "curve25519-js": "^0.0.4",
    "libsodium-wrappers": "^0.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.1",
    "@vitest/coverage-v8": "^4.0.18",
    "libsignal": "git+https://github.com/WhiskeySockets/libsignal-node.git",
    "cpy-cli": "^7.0.0",
    "cross-env": "^10.1.0",
    "fast-check": "^4.5.3",
    "protobufjs-cli": "^2.0.0",
    "rimraf": "^6.1.3",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "sideEffects": false,
  "keywords": [
    "signal-protocol",
    "libsignal",
    "double-ratchet",
    "x3dh",
    "end-to-end-encryption",
    "secure-messaging",
    "forward-secrecy",
    "post-compromise-security",
    "prekey",
    "session-encryption",
    "x25519",
    "ed25519",
    "protobuf",
    "libsodium",
    "typescript"
  ]
}
